    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "axios": "^0.21.1",
    "jest": "^26.1.0",
    "standard-reporter": "^1.0.5",
    "standardx": "^5.0.0",
    "ts-jest": "^26.1.3",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.52",
    "typescript": "^3.9.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Karaja-Naji/tui-task.git"
